    {
      "id" : "05F5D96B-2331-443E-A711-1E892DA4BC29",
      "showMonsterSprite" : false,
      "isFromModel" : true,
      "showCharacterSprite" : false,
      "content" : "The deeper you delve, the colder the air grows. Shadows stretch across the rocky floor like grasping fingers, weaving an intricate dance that seems to pulse with a life of their own. An unsettling sense of familiarity lingers in the air, as if you've tread this path before."
    },
    {
      "id" : "472597E2-9177-4896-9545-F36B124F45F9",
      "showMonsterSprite" : false,
      "isFromModel" : false,
      "showCharacterSprite" : false,
